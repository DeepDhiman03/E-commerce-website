tCartSubTotal = () => {\n    return cartItems\n      .reduce((price, item) => price + item.price * item.qty, 0)\n      .toFixed(2);\n  };\n\n  const handleProceedBtn = () => {\n    alert(\"Functionality pending please stay tune, will be add soon.\");\n  };\n\n  if (loginInfo.loading) return <h1>Loading.....</h1>;\n  else if (!loginInfo.loading && loginInfo.isLogin)\n    return (\n      <>\n        <div className=\"cartscreen\">\n          <div className=\"cartscreen__left\">\n            <h2>Shopping Cart</h2>\n\n            {cartItems.length === 0 ? (\n              <div>\n                Your Cart Is Empty <Link to=\"/\">Go Back</Link>\n              </div>\n            ) : (\n              cartItems.map((item) => (\n                <CartItem\n                  key={item.product}\n                  item={item}\n                  qtyChangeHandler={qtyChangeHandler}\n                  removeHandler={() => removeFromCartHandler(item)}\n                />\n              ))\n            )}\n          </div>\n\n          <div className=\"cartscreen__right\">\n            <div className=\"cartscreen__info\">\n              <p>Subtotal ({getCartCount()}) items</p>\n              <p>${getCartSubTotal()}</p>\n            </div>\n            <div>\n              <button\n                title=\"Functionality need to be add.\"\n              >\n                Proceed To Checkout\n              </button>\n            </div>\n          </div>\n        </div>\n      </>\n    );\n};\n\nexport default CartScreen;\n","D:\\projects\\e-commerce-store\\e-comerce-frontend\\src\\components\\Backdrop.js",[],"D:\\projects\\e-commerce-store\\e-comerce-frontend\\src\\components\\SideDrawer.js",[],"D:\\projects\\e-commerce-store\\e-comerce-frontend\\src\\screens\\ProductScreen.js",[],"D:\\projects\\e-commerce-store\\e-comerce-frontend\\src\\redux\\actions\\cartActions.js",["125"],"import * as actionTypes from '../constants/cartConstants'\nimport axios from 'axios'\nimport {Api} from '../../